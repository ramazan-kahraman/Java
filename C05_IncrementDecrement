package ders04_matematikseli≈ülemler_incrementDecrement;

public class C05_IncrementDecrement {
    public static void main(String[] args) {

        int sayi=10;
        sayi=sayi+3;
        sayi +=3;


        int s=10;
        s=s+1;
        s+=1;
        s++;
        ++s;

        int t=10;
        t=t-1;
        t-=1;
        t--;
        --t;





    }
}
